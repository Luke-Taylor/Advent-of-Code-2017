        Return "multiline"
    End Function

    Public Overrides Function GetText(Part As String) As String
        If (Part = "a") Then
            Return "Somehow, a network packet got lost and ended up here. It's trying to follow a routing diagram (your puzzle input), but it's confused about where to go." + vbCrLf _
                + "Its starting point is just off the top of the diagram. Lines (drawn with |, -, and +) show the path it needs to take, starting by going down onto the only line connected to the top of the diagram. It needs to follow this path until it reaches the end (located somewhere within the diagram) and stop there." + vbCrLf _
                + "Sometimes, the lines cross over each other; in these cases, it needs to continue going the same direction, and only turn left or right when there's no other option. In addition, someone has left letters on the line; these also don't change its direction, but it can use them to keep track of where it's been." + vbCrLf _
                + "Given this diagram, the packet needs to take the following path:" + vbCrLf _
                + "Starting at the only line touching the top of the diagram, it must go down, pass through A, and continue onward to the first +." + vbCrLf _
                + "Travel right, up, and right, passing through B in the process." + vbCrLf _
                + "Continue down (collecting C), right, and up (collecting D)." + vbCrLf _
                + "Finally, go all the way left through E and stopping at F." + vbCrLf _
                + "Following the path to the end, the letters it sees on its path are ABCDEF." _
                + "The little packet looks up at you, hoping you can help it find the way. What letters will it see (in the order it would see them) if it follows the path? (The routing diagram is very wide; make sure you view it without line wrapping.)"
        Else
            Return "The packet is curious how many steps it needs to go." + vbCrLf _
             + "How many steps does the packet need to go?"
        End If
    End Function

    Public Overrides Function PartA(PuzzleInput As String) As String
        Dim lines As List(Of String)
        Dim letters As String = ""
        Dim x As Integer = 0
        Dim y As Integer = 1
        Dim currentWire As String = "|"
        Dim done As Boolean = False
        Dim dir As Integer = 0 '0 = down, 1 = right, 2 = up, 3 = left

        Try
            lines = ParseInput(PuzzleInput)

            x = lines(0).IndexOf("|")
            While (Not done)
                currentWire = lines(y)(x)

                Select Case (currentWire)

                    Case "|"
                        If (dir = 0) Then
                            y += 1
                        ElseIf (dir = 2) Then
                            y -= 1
                        Else
                            If (Not lines(y)(x - 1) = " " And dir = 3) Then
                                x -= 1
                            ElseIf (Not lines(y)(x + 1) = " " And dir = 1) Then
                                x += 1
                            End If
                        End If

                    Case "+"
                        If (dir = 0 Or dir = 2) Then
                            If (Not lines(y)(x + 1) = " ") Then
                                dir = 1
                                x += 1
                            ElseIf (Not lines(y)(x - 1) = " ") Then
                                dir = 3
                                x -= 1
                            End If
                        Else
                            If (Not lines(y - 1)(x) = " ") Then
                                dir = 2
                                y -= 1
                            ElseIf (Not lines(y + 1)(x) = " ") Then
                                dir = 0
                                y += 1
                            End If
                        End If

                    Case "-"
                        If (dir = 1) Then
                            x += 1
                        ElseIf (dir = 3) Then
                            x -= 1
                        Else
                            If (Not lines(y + 1)(x) = " " And Not dir = 2) Then
                                y += 1
                                dir = 0
                            ElseIf (Not lines(y - 1)(x) = " " And Not dir = 0) Then
                                y -= 1
                                dir = 2
                            End If
                        End If

                    Case " "
                        done = True

                    Case Else
                        letters += currentWire

                        If (dir = 0) Then
                            y += 1
                        ElseIf (dir = 1) Then
                            x += 1
                        ElseIf (dir = 2) Then
                            y -= 1
                        ElseIf (dir = 3) Then
                            x -= 1
                        End If

                End Select
            End While

            Return letters
        Catch ex As Exception
            Throw New Exception("Error getting solution for part A", ex)
        End Try
    End Function

    Public Overrides Function PartB(PuzzleInput As String) As String
        Dim lines As List(Of String)
        Dim letters As String = ""
        Dim x As Integer = 0
        Dim y As Integer = 1
        Dim currentWire As String = "|"
        Dim done As Boolean = False
        Dim dir As Integer = 0 '0 = down, 1 = right, 2 = up, 3 = left
        Dim steps As Integer = 0

        Try
            lines = ParseInput(PuzzleInput)

            x = lines(0).IndexOf("|")
            While (Not done)
                steps += 1
                currentWire = lines(y)(x)

                Select Case (currentWire)

                    Case "|"
                        If (dir = 0) Then
                            y += 1
                        ElseIf (dir = 2) Then
                            y -= 1
                        Else
                            If (Not lines(y)(x - 1) = " " And dir = 3) Then
                                x -= 1
                            ElseIf (Not lines(y)(x + 1) = " " And dir = 1) Then
                                x += 1
                            End If
                        End If

                    Case "+"
                        If (dir = 0 Or dir = 2) Then
                            If (Not lines(y)(x + 1) = " ") Then
                                dir = 1
                                x += 1
                            ElseIf (Not lines(y)(x - 1) = " ") Then
                                dir = 3
                                x -= 1
                            End If
                        Else
                            If (Not lines(y - 1)(x) = " ") Then
                                dir = 2
                                y -= 1
                            ElseIf (Not lines(y + 1)(x) = " ") Then
                                dir = 0
                                y += 1
                            End If
                        End If

                    Case "-"
                        If (dir = 1) Then
                            x += 1
                        ElseIf (dir = 3) Then
                            x -= 1
                        Else
                            If (Not lines(y + 1)(x) = " " And Not dir = 2) Then
                                y += 1
                                dir = 0
                            ElseIf (Not lines(y - 1)(x) = " " And Not dir = 0) Then
                                y -= 1
                                dir = 2
                            End If
                        End If

                    Case " "
                        done = True

                    Case Else
                        letters += currentWire

                        If (dir = 0) Then
                            y += 1
                        ElseIf (dir = 1) Then
                            x += 1
                        ElseIf (dir = 2) Then
                            y -= 1
                        ElseIf (dir = 3) Then
                            x -= 1
                        End If

                End Select
            End While

            Return steps
        Catch ex As Exception
            Throw New Exception("Error getting solution for part A", ex)
        End Try
    End Function
